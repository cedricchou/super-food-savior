<%- include('../partials/header') %>

<h1>Welcome to the message board</h1>

<div class="row">
  <% for (let i = myMessages.length-1; i >= 0; i-- ) { %>
    <div class="card col-lg-6">
      <div class="card-body">
      <p class="card-text"><%= myMessages[i].content %></p>
      </div>
    </div>
  <% } %>
</div>

<%- include('../partials/footer') %>
