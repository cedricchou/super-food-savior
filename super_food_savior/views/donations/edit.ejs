<%- include('../partials/header') %>


    <div id="map-index" style="width: 500px; height: 400px;"></div>
  <script
    src="https://maps.googleapis.com/maps/api/js?key=<%=GMAP_KEY%>"
    type="text/javascript"
    >
  </script>

  <script>
  let map;

  let data = <%- JSON.stringify(usersGeocode) %>

  function initialize() {

    let mapOptions = {
      zoom: 7,
      center: new google.maps.LatLng(49.246292, -123.116226)
    };

    // Creating the map
    map = new google.maps.Map(document.getElementById('map-index'), mapOptions);

    for(let i = 0; i < data.length; i++) {

      // Adding a new marker for the object
      let marker = new google.maps.Marker({
        position: new google.maps.LatLng(data[i].lat,data[i].lng),
        map: map
      });
    }
  }

    // Initialize the map
    google.maps.event.addDomListener(window, 'load', initialize);
  </script>





